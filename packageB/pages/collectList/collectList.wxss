.avatar-emote-view image{width: 230rpx;height: 230rpx;}

.delete-box{
  position: absolute;
  height: 82rpx;
  width: 100%;
  background: rgba(255,255,255,0.9);
  box-shadow: 0 -1px 0 0 #ECECEC;
  bottom: 0;
  left: 0;
  
}
.delete-box > view{height: 100%;position: absolute;left: 0;right: 0;width: 100%;}
.delete-entry{
  flex: 1;
  background: url(data:image/png;base64,) center center no-repeat;background-size: 36rpx 45rpx;
}
.delete-ctrl{
  display: flex;
  justify-content:space-between;
}
.delete-ctrl > view{
  height: 100%;
}
.delete-cancle,.delete-submit{
  width: 160rpx;
  vertical-align: middle;
  line-height: 82rpx;
    font-size: 32rpx;
  text-align: center;
}

.delete-cancle{

color: #3C3D40;
}
.delete-submit{
  color: #3399CB;
}