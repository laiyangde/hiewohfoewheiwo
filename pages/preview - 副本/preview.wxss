.ad-border{
  height: 42rpx;
  width: 500rpx;
  margin: 0 auto 12rpx;
  /* border-bottom: 1rpx dashed #C3C3C3; */
  position: relative;
  text-align: center;
  font-size: 20rpx;
  color: #949494;
  line-height: 42rpx;
}
.ad-border::before,.ad-border::after{
  content: '';
  display: block;
  width: 190rpx;
  border-bottom: 1px dashed #C3C3C3;
  position: absolute;
  top: 50%;
  left: 0;
}
.ad-border::after{
  left: auto;
  right: 0;
}


