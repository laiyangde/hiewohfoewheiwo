swiper.active{background-color:#f2f2f2}


swiper .loading {
    position: absolute;
    top: 40%;
    left: 50%;
    transform: translate(-50%, -50%);
    color: #fff;
    width: 60rpx;
    height: 60rpx;
    background: none;
}

.avatar-emote-preview swiper-item {
    display: flex;
    justify-content: center;
    align-items: center;
    overflow: visible;
}

.open-buy-btn-box {
    height: 120rpx;
    justify-content: space-around;
    align-items: center;
}
.open-buy-btn-box.isShareUnlock::after{
  content: '';
  display: block;
  height: 50rpx;
  width: 1rpx;
  background: #FFFFFF;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%)
}

.open-buy-btn-box button {
    background: #239efe;
    border-radius: 200rpx;
    width: 294rpx;
    height: 80rpx;
    font-size: 32rpx;
    color: #fff;
    line-height: 80rpx;
    position: relative;
    padding-left: 20rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: center;
    align-items: center;
}
.open-buy-btn-box button:last-child{
  background: #F5A623;
}

.open-buy-btn-box image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 10rpx
}

/* 赞赏面板 */

.buyPanel-container {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.6);
    z-index: 999;
}

.buyPanel {
    width: 564rpx;
    height: 564rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background-color: #fff;
    border-radius: 12rpx;
    box-sizing: border-box;
    padding: 40rpx 40rpx 30rpx;
}

.closeBuyPanel {
    width: 58rpx;
    height: 58rpx;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
}

.author-image {
    width: 112rpx;
    height: 112rpx;
    margin: 0 auto; 
    display: block;
    border-radius: 50%;
}

.author-nickName {
    font-size: 28rpx;
    color: #6a7883;
    display: block;
    text-align: center;
    margin: 14rpx 0 40rpx;
    letter-spacing: 1rpx;
}

.copyright {
    font-size: 28rpx;
    color: #4c4c4c;
    letter-spacing: 1rpx;
    text-indent: 1.2em;
    position: relative;
}

.copyright image {
    width: 26rpx;
    height: 26rpx;
    position: absolute;
    top: 6rpx;
    left: 0;
}

.pay-btn {
    background: #239efe;
    border-radius: 200rpx;
    font-size: 32rpx;
    color: #fff;
    letter-spacing: 1.15rpx;
    width: 484rpx;
    height: 88rpx;
    margin: 60rpx auto 30rpx;
    line-height: 88rpx;
}

.buyPanel .tip {
    font-size: 24rpx;
    color: #949494;
    letter-spacing: 0.86rpx;
    text-align: center;
    display: block;
}

.toINOTCH-btn{
  position: fixed;
  left: 50%;
  transform: translateX(-50%);
  bottom: 140rpx;
  width: 250rpx;
  height: 76rpx;
}
.ipx .toINOTCH-btn{
  bottom: 200rpx;
}

/* 弹窗广告流量主 */

.p-ad{
  position: fixed;
