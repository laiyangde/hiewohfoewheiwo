/* 引导图 */
.guide-container{
  width: 100%;
  height: 100%
}
.guide-container .guide{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  flex-direction:column;
  box-sizing: border-box;
  padding-bottom: 50rpx;
  pointer-events: none;
}

.guide-container .guide1 text{
    color: #fff;
    display: block;
    text-align: center;
}
.guide-container .guide1 text:nth-of-type(1){
  font-size: 44rpx;
  font-weight: bold;
  line-height: 74rpx;
}
