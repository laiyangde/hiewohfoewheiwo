  height: 98rpx;
  right: 0;
  bottom: 0;
}
.guide-container .guide1 .highLighter view{
  justify-content:center;
  align-items:center;
  flex-direction:column;
  display: flex;
  margin: 0 auto;
  height: 100%;
  width: 98rpx;
  background-color: #d9d9de;
  border-radius: 50%;
}
.guide-container .guide1 .highLighter view image{
  width:46rpx;
  height:46rpx;
  display:block;
}
.guide-container .guide1 .highLighter view text{
  font-size:28rpx;
  color:#4C4C4C;
  line-height:30rpx;
}
.ipx .guide-container .guide1 .highLighter{
  bottom: 48rpx;
}
.guide-container .guide1 .guide-pointer{
  width: 239rpx;
  height: 282rpx;
