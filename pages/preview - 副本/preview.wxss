.p-ad-content{
  background: #FFFFFF;
  border-radius: 12rpx;
  width: 564rpx;
  padding: 50rpx 20rpx;
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-60%)
}

.p-ad-content>text{
  font-size: 32rpx;
  color: rgba(35,158,254,1);
  text-align: center;
  line-height: 46rpx;
  display: block;
}
.p-ad-content>text:first-child{
 color: #6A7883;
}
.p-ad-view{
  position: relative;
  margin-top: 40rpx;
  height: 150rpx;
}

.p-ad-view .p-ad-btn{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width: 430rpx;
  height: 71rpx;
  background: rgba(35,158,254,0.8);
  border-radius: 35.5rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  pointer-events: none;
  z-index: 2
}
.p-ad-view .p-ad-btn image{
  width: 21.4rpx;
  height: 25rpx;
}
.p-ad-content >image{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: -5rpx;
  top: -5rpx;
  padding: 20rpx;
}
.p-ad-view .p-ad-btn text{
  font-size: 28rpx;
  color: #FFFFFF;
  display: block;
  margin-left: 20rpx;
}

.p-ad-view .p-ad-wrap{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%) scale(0.698);
  width: 750rpx;
