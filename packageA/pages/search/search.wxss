.search-btn::after {
    border: 0;
}

.clearSearchKey {
    /* background: url(data:image/png;base64,) center center no-repeat; */
    /* background-size: 28rpx 28rpx; */
    width: 72rpx;
    height: 100%;
    position: absolute;
    top: 0;
    right: 0;
    z-index: 2;
    display: flex;
    justify-content: center;
    align-items: center
}
.clearSearchKey image{
  width: 28rpx;
  height: 28rpx;
  display: block
}
.search-box .search-input .close {
    background: url(data:image/png;base64,) 60rpx center no-repeat;
    background-size: 28rpx 28rpx;
    width: 108rpx;
    height: 78rpx;
    position: absolute;
    top: -10rpx;
    right: 0;
    z-index: 999;
}

.search-box .search-input .placeholder {
    font-size: 28rpx;
    color: #8f8f8f;
    line-height: 1;
}

.search-box .search-btn {
    box-sizing: border-box;
    padding: 15rpx 0 15rpx 57rpx;
    line-height: 1;
    text-align: left;
}

.search-box .search-input input {
    width: 85%;
    font-size: 28rpx;
    height: 1.1em;
    min-height: 1.1em;
    padding: 15rpx 0;
    line-height: 1.1;
}

.search-box .search-cancle {
    font-size: 32rpx;
    color: #2e93c4;
    line-height: 58rpx;
    margin-left: 25rpx;
    background-color: #fff;
}

.search-page {
    background-color: #fff;
}

.search-page .key-list, .search-page .search-list {
    height: 100%;
}

.search-keys {
    padding: 0 0rpx 0 20rpx;
}
.base-top-nav{
  border-bottom: 1rpx solid #E6E6E6;
  background-color: #fff;
  z-index: 99
}
.recent-search {
    /* border-bottom: 20rpx solid #f2f2f2; */
    padding-bottom: 20rpx;
    position: relative;
}
.recent-search::before{
  content: '';
  width: 710rpx;
  height: 2rpx;
  background: #E6E6E6;
  display: block;
  position: absolute;
  bottom: 0;
  left: 20rpx;
}
.search-keys text {
    font-size: 30rpx;
    color: #6a7883;
    line-height: 3;
}

.key-btns {
    display: flex;
    flex-wrap: wrap;
    position: relative;
    
}
.hot-search .key-btns{
  min-height: 160rpx;
}

.key-btns button {
    font-size: 28rpx;
    width: 160rpx;
    height: 60rpx;
    padding: 0 8rpx;
    line-height: 60rpx;
    color: #6a7883;
    margin: 0 20rpx 20rpx 0;
    background-color: #fff;
    border: 2rpx solid #CDCDCD;
    border-radius: 6rpx;
    box-sizing: border-box;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    
}
.hot-search .key-btns button:nth-child(1),.hot-search .key-btns button:nth-child(4){
margin-left: 180rpx;
}
