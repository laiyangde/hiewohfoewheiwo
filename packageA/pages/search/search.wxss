/* pages/search/search.wxss */
.suspension-btn{
  bottom: 110rpx;
}
.suspension-btn.iphonex{
  bottom: 178rpx;
}
.content{
  padding-top: 16rpx;
}

.keywrod-wrap .multi-fn-btn{
  display: flex;
  box-shadow: 0 6rpx 20rpx 0 rgba(0,0,0,0.10);
  border-radius: 3rpx;
  width: 710rpx;
  margin: 20rpx auto;
  padding: 20rpx;
}
.keywrod-wrap .multi-fn-btn image{
  width: 120rpx;
  height: 120rpx;
  margin-right: 20rpx;
}
.keywrod-wrap .multi-fn-btn .right{
  flex: 1;
}
.keywrod-wrap .multi-fn-btn .right text{
  text-align: left;
  display: block;
}
.keywrod-wrap .multi-fn-btn .right text:first-child{
    font-size: 32rpx;
    font-weight: 500;
    line-height: 45rpx;
}
.keywrod-wrap .multi-fn-btn .right text:last-child{
  font-size: 24rpx;
  line-height: 34rpx;
  margin-top: 14rpx;
}
.indexEntry{
  top: auto;
  bottom: 40rpx;
  left: 50%;
  transform: translateX(-50%)
}
