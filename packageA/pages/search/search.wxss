.avatar-emote-view image {
    width: 230rpx;
    height: 230rpx;
}

.content-wrap,.result-wrap,.keywrod-wrap{height: 100%;}

.content-wrap {
    position: relative;
}

.keywrod-wrap {
    
}

.result-wrap {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background-color: #fff;
  box-sizing: border-box;
}

.search-wrap .search-box input {
    height: 100%;
    width: 100%;
    vertical-align: middle;
    box-sizing: border-box;
}
.search-wrap .search-box {
  font-size: 28rpx;
  color: #737373;
  line-height: 66rpx;
  background: #f2f2f2 url(data:image/png;base64,) 20rpx center no-repeat;
  box-sizing: border-box;
  padding-left: 64rpx;
  padding-right: 72rpx;
  background-size: 36rpx 36rpx;
  display: flex;
  align-items: center;
  flex: 1;
  position: relative;
  height: 66rpx;
  border-radius: 33rpx;
}
