  /* 排序 */
.order-mask{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.2);
  z-index: 100;
}

.order-select-btns{
  position: fixed;
  bottom: 116rpx;
  left: 300rpx;
  box-shadow: 0 0 12rpx 0 rgba(0,0,0,0.27);
  width: 306rpx;
  height: 255rpx;
  border-radius: 8rpx;
  background: #FFFFFF;
  transition: all 0.3s;
  transform-origin: 25% 110%;
  transform:scale(0); 
  z-index: 100
}

.order-select-btns.active{
  transform: scale(1)
}
.order-select-btns::after{
  width: 42rpx;
  height: 42rpx;
  content: '';
  display: block;
  background: #fff;
  box-shadow: 0 0 12rpx 0 rgba(0,0,0,0.27);
  border-radius: 10rpx;
  position: absolute;
  bottom: -12rpx;
  left: 56rpx;
  transform: rotate(45deg);
  pointer-events: none
}


.order-select-btns .order-select-wrap{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #FFFFFF;
  z-index: 1;
  border-radius: 8rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
}


.order-select-btns .order-select-wrap button{
  font-size: 32rpx;
  color: #4C4C4C;
  letter-spacing: 0.38rpx;
  height: 84rpx;
  line-height: 84rpx;
  border-top: 1rpx solid #E3E3E3;
  text-align: left;
}
.order-select-btns .order-select-wrap button.active{
  background: url(https://minibizhi.313515.com/BiZhiStatic/icon-all-bottom-select.svg) right center no-repeat;
  background-size: 29rpx 22rpx;
  font-weight: 500;
}

.order-select-btns .order-select-wrap button:first-child{
  border-color: transparent;
}


