}


.scroll-nav scroll-view {
  width: 100%;
  height: 56rpx;
  white-space: nowrap;
  /* box-shadow: 0 4rpx 20rpx 0 #e6e6e6; */
}

.scroll-nav scroll-view  button {
  display: inline-block;
  margin-right: 30rpx;
  /* padding: 0 8rpx; */
  height: 100%;
  overflow: visible;
  /*width: 800rpx;*//*width: auto;*/
}
.scroll-nav scroll-view  button:last-child{
  margin-right: 0
}

.scroll-nav scroll-view button .nav-text {
  /*width:50rpx;*/
  padding: 0rpx 34rpx;
  font-size: 26rpx;
  color: #818181;
  /* border-bottom: 4rpx solid transparent; */
  box-sizing: border-box;
  height: 100%;
  line-height: 56rpx;
  display: block;
  background: #F7F7F7;
  border-radius: 28rpx;
  letter-spacing: 0.4rpx;
}
.scroll-nav scroll-view button .nav-red-point{
  position: absolute;
  display: block;
  width: 14rpx;
  height: 14rpx;
  background-color: #ee3c2e;
  right: 8rpx;
  top: 50%;
  transform: translateY(-50%);
  border-radius: 50%;
}
.scroll-nav scroll-view button .active {
  color: #239EFE;
  background: rgba(35,158,254,0.20);
}

.scroll-nav scroll-view button:first-child {
  padding-left: 16rpx;
}

.scroll-nav scroll-view button:last-child {
  padding-right: 16rpx;
}

.scroll-nav .show-nav-btn{
  width: 104rpx;
  height: 96rpx;
  position: absolute;
  right: 0;
  top: 0;
  background-color: rgba(255, 255, 255, 0.9);
  z-index: 2;
}
.scroll-nav .show-nav-btn::before{
  content: '';
  width: 14rpx;
  height: 14rpx;
   border:2rpx solid #6A7883; 
   border-bottom-color: transparent; 
   border-left-color: transparent; 
  display: block;
  top: 58%;
  left:50%;
  border-radius: 3rpx;
  position: absolute;
  margin-left: -8rpx;
  margin-top: -8rpx;
  transform-origin: 75% 25%;
  transform:rotate(135deg);   
  transition: all 0.3s;
   /* background-color: red  */
}
.scroll-nav .show-nav-btn.active::before{
  transform:rotate(-45deg);
}
.scroll-nav .nav-view{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  height: 100%;
  z-index: 2;
  transform: translate3d(0,-100%,0);
  transition: all 0.3s;
  display: flex;
  flex-direction: column;
}


.scroll-nav .nav-view.active{
  transform: translate3d(0,0,0)
}

.scroll-nav .nav-view .top{
  height: 96rpx;
  font-size: 30rpx;
  color: #6A7883;
  line-height: 96rpx;
  padding-left: 40rpx;
  border-bottom:1rpx solid #e8e8e8;
}

.scroll-nav .nav-view .bottom{
  display: flex;
  flex-wrap: wrap;
  /* justify-content: space-between; */
  padding-left:44rpx;
  overflow: scroll;
  flex: 1;
  -webkit-overflow-scrolling: touch;
align-content:flex-start;

}
.scroll-nav .nav-view .bottom button{
  margin-right: 60rpx;
  margin-top: 26rpx;
}
.scroll-nav .nav-view .bottom button:nth-child(4n){
  margin-right:0
}
.scroll-nav .nav-view .bottom image{
  width: 120rpx;
